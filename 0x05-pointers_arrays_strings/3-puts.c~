#include <stdio.h>
/**
 * _puts(char *str) - This function prints a string, followed by a newline, to stdout
 *
 * Return: string
 */
void _puts(char *str)
{
  while (*str != '\0')
    {
      putchar(*str);
      str++;
    }
  putchar('\n');
}
int main()
{
  char mySting[] = "I do not fear computers. I fear the lack of them - Isaac Asimov";
  _puts(mySting);
  return 0;
}
